<!DOCTYPE html>
<html>

<head>
    <%- include('head-inc'); %>
</head>

<body>
    <%- include('nav-main'); %>
    <div class="container">
        <div class="row">
            <table class="table">
                <tr>
                    <th style="text-align: center">Archivo</th>
                    <th style="text-align: center">Nombre</th>
                    <th style="text-align: center">Acciones</th>
                </tr>
                    <% for(var i=0; i < imageUrls.length; i++) { %>
                        <div class="col-md-4">
                            <tr>
                                <% if(imageNames[i].search(/.png/i)>-1) { %>
                                    <td><img src="<%=imageUrls[i]%>" class="rounded mx-auto d-block" alt="Lights" style="width:20%"></td>
                                <% } else { %>
                                    <td><img src="data:image/png;base64,"  class="rounded mx-auto d-block" alt="Lights" style="width:20%"></td>
                                <%}%>
                                <td><%=imageNames[i]%></td>
                                <td class="dropdown col-7">
                                    <p class="text-right">â˜°</p>
                                    <div class="dropdown-content">
                                            <form action="/doc/delete" method="post">
                                                <input type="hidden" id="nameKey" name="nameKey" value="<%=imageNames[i]%>">
                                                <input class="btn btn-light btn-block" type="submit" value="Descargar">
                                            </form>
                                            <form action="/doc/download" method="post">
                                                <input type="hidden" id="nameKey" name="nameKey" value="<%=imageUrls[i]%>">
                                                <input class="btn btn-light btn-block" type="submit" value="Recuperar">
                                            </form>
                                            <form action="/doc/deltrash" method="post">
                                                <input type="hidden" id="nameKey" name="nameKey" value="<%=imageUrls[i]%>">
                                                <input class="btn btn-light btn-block" type="submit" value="Eliminar">
                                            </form>
                                    </div>
                                </td>
                            </tr>

                                    
                        </div>
                    <% } %>
            </table>
        </div>
    </div>
</body>

</html>
